                    app:layout_constraintWidth_percent=".95">

                    <ImageView
                        android:id="@+id/logoutCImgg"
                        android:layout_width="45dp"
                        android:layout_height="45dp"
                        android:layout_margin="10dp"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/circle_profile_background"
                        android:padding="12dp"
                        android:src="@drawable/logout"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:tint="@color/logo_color" />


                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/text_size_20"
                        android:gravity="center_vertical"
                        android:text="@string/logout"
                        android:textColor="@color/black"
                        android:textSize="@dimen/normal_txt_size"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toBottomOf="@+id/logoutCImgg"
                        app:layout_constraintStart_toEndOf="@+id/logoutCImgg"
                        app:layout_constraintTop_toTopOf="@+id/logoutCImgg"
                        app:layout_constraintWidth_percent=".7" />

                    <ImageView
                        android:id="@+id/logoutA"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_margin="10dp"
                        android:src="@drawable/forward_arrow"
                        app:layout_constraintBottom_toBottomOf="@+id/logoutCImgg"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/logoutCImgg"
                        app:tint="@color/black" />
                </androidx.constraintlayout.widget.ConstraintLayout>


            </androidx.constraintlayout.widget.ConstraintLayout>
